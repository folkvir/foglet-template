{
  "name": "foglet-template",
  "version": "1.0.0",
  "description": "Template using foglet-core module",
  "main": "index.js",
  "scripts": {
    "lint": "standard --verbose \"./lib/**/*.js\" \"./tests/**/*.js\" | snazzy",
    "fix": "standard --fix --verbose \"./lib/**/*.js\" \"./tests/**/*.js\" | snazzy",
    "build:dev": "webpack --watch --config ./configs/webpack-config.js --mode development",
    "build:prod": "webpack --config ./configs/webpack-config.min.js --mode production",
    "test": "mocha tests/*-test.js --exit"
  },
  "directories": {
    "lib": "lib",
    "test": "tests",
    "bin": "bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/folkvir/foglet-template.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/folkvir/foglet-template/issues"
  },
  "homepage": "https://github.com/folkvir/foglet-template#readme",
  "dependencies": {
    "debug": "^4.0.1",
    "foglet-core": "^5.1.1",
    "lodash.merge": "^4.6.1",
    "sigma": "^1.2.1",
    "tman-wrtc": "^1.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "mocha": "^5.2.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "wrtc": "^0.2.1"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
